cipher = CaesarCipher(15)

words = ['civilğŸ˜€', 'soğŸ˜', 'regionâ˜º', 'beatâ˜º', 'artistğŸ˜', 'choiceğŸ™ƒ', 'includeğŸ¤­', 'degreeğŸ˜', 'pushğŸ¤ª', 'sideğŸ˜', 'sizeğŸ¤¥',
         'policyğŸ¤¨', 'ğŸ¤¨ğŸ¤¥ğŸ˜ğŸ¤ªğŸ˜ğŸ¤­ğŸ™ƒğŸ˜â˜ºğŸ˜ğŸ˜€']

