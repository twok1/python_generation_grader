data1 = [0, 1, 1, 0, 0, 1]
data2 = [1, 1, 1, 1, 1]

bitarray1 = BitArray(data1)
bitarray2 = BitArray(data2)

try:
    print(bitarray1 | bitarray2)
except TypeError:
    print('Списки должны быть равной длины')

try:
    print(bitarray1 & bitarray2)
except TypeError:
    print('Списки должны быть равной длины')